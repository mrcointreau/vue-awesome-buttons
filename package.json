{
  "name": "vue-awesome-buttons",
  "version": "1.0.0",
  "private": false,
  "main": "dist/vue-awesome-buttons.umd.min.js",
  "unpkg": "dist/vue-awesome-buttons.umd.min.js",
  "jsdelivr": "dist/vue-awesome-buttons.umd.min.js",
  "author": "mrcointreau",
  "license": "MIT",
  "description": "Awesome Vue.js button components",
  "files": [
    "dist/*",
    "src/*"
  ],
  "homepage": "https://github.com/mrcointreau/vue-awesome-buttons#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrcointreau/vue-awesome-buttons.git"
  },
  "bugs": {
    "url": "https://github.com/mrcointreau/vue-awesome-buttons/issues"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue-awesome-buttons src/main.js",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "keywords": [
    "vue",
    "awesome",
    "buttons"
  ]
}
